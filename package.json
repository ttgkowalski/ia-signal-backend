  "main": "index.js",
  "scripts": {
    "dev": "dotenv -e .env.dev -- tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "migrate:make": "bun run scripts/migrate-make.ts",
    "migrate:latest": "bun run scripts/migrate.ts latest",
    "migrate:latest:prod": "bun run scripts/migrate.ts latest --env=production",
    "migrate:rollback": "bun run scripts/migrate.ts rollback",
    "migrate:rollback:all": "bun run scripts/migrate.ts rollback --all",
    "migrate:up": "bun run scripts/migrate.ts up",
    "migrate:down": "bun run scripts/migrate.ts down",
    "migrate:list": "bun run scripts/migrate.ts list",
    "up:dev": "docker compose --env-file .env.dev up -d",
    "up:test": "docker compose --env-file .env.test up -d",
    "up:prod": "docker compose --env-file .env.prod up -d",
    "down:dev": "docker compose --env-file .env.dev down",
    "down:test": "docker compose --env-file .env.test down",
    "down:prod": "docker compose --env-file .env.prod down"
  },
